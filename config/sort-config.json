{
  "directories": {
    "tankoubon": "单行本",
    "doujinshi": "短篇",
    "guro": "猎奇"
  },
  "tag_rules": [
    {
      "tag": "female:guro",
      "category": "guro",
      "priority": 1,
      "description": "Guro content - highest priority"
    },
    {
      "tag": "other:tankoubon",
      "category": "tankoubon",
      "priority": 2,
      "description": "Full volume manga collections"
    },
    {
      "tag": "other:goudoushi",
      "category": "doujinshi",
      "priority": 3,
      "use_circle_name": true,
      "description": "Anthology works - use circle name instead of author"
    }
  ],
  "default_category": "doujinshi",
  "filename_patterns": {
    "circle_author": "\\[([^(]+)\\(([^)]+)\\)\\]",
    "event_circle_author": "\\([^)]+\\)\\[([^(]+)\\(([^)]+)\\)\\]",
    "author_only": "\\[([^\\]]+)\\]",
    "event_author": "\\([^)]+\\)\\[([^\\]]+)\\]"
  },
  "options": {
    "create_author_dirs": true,
    "skip_existing": true,
    "log_file": "sort_log.txt",
    "temp_dir": "/tmp",
    "sanitize_names": true
  }
}