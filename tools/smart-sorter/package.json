{
  "name": "smart-archive-sorter",
  "version": "1.0.0",
  "description": "AI-powered archive sorter for ExHentai/E-Hentai content with interactive classification",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && cp -r src/config dist/",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "bin": {
    "smart-sort": "dist/index.js"
  },
  "keywords": [
    "archive",
    "sorter",
    "exhentai",
    "ai",
    "classification"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "yauzl": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/yauzl": "^2.10.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}